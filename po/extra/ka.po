#: data/installer.metainfo.xml.in:9
msgid "Installer"
msgstr "დაყენება"

#: data/installer.metainfo.xml.in:10 data/io.elementary.installer.desktop.in:3
msgid "Install elementary OS"
msgstr "Elementary OS-ის დაყენება"

#: data/installer.metainfo.xml.in:12
msgid "Step by step installer for elementary OS itself."
msgstr "პროგრამა, რომელიც elementary OS-ს ნაბიჯ-ნაბიჯ დაგაყენებინებთ."

#: data/installer.metainfo.xml.in:52
msgid "elementary, Inc."
msgstr "elementary, Inc."

#: data/installer.metainfo.xml.in:59 data/installer.metainfo.xml.in:70
#: data/installer.metainfo.xml.in:85 data/installer.metainfo.xml.in:95
#: data/installer.metainfo.xml.in:105
msgid "Improvements:"
msgstr "გაუმჯობესებები:"

#: data/installer.metainfo.xml.in:61
msgid "Use large icons in views"
msgstr "დიდი ხატულების ჩვენება"

#: data/installer.metainfo.xml.in:62
msgid "Add a custom icon for Before Installing view"
msgstr "ხატულის დამატება ხედის დაყენებამდე"

#: data/installer.metainfo.xml.in:63 data/installer.metainfo.xml.in:75
#: data/installer.metainfo.xml.in:88 data/installer.metainfo.xml.in:98
#: data/installer.metainfo.xml.in:109
msgid "Updated translations"
msgstr "განახლებული თარგმანები"

#: data/installer.metainfo.xml.in:72
msgid "Show an infobar on battery instead of switching to a different page"
msgstr "სხვა გვერდზე გადართვის მაგიერ ელემენტის ინფორმაციის ჩვენება"

#: data/installer.metainfo.xml.in:73
msgid "Show all system requirements warnings on a single page"
msgstr "სისტემური მოთხოვნების შესახებ გაფრთხილებების ერთ გვერდზე ჩვენება"

#: data/installer.metainfo.xml.in:74
msgid "Warn about installing developer builds"
msgstr "გაფრთხილება არასტაბილური აგებების შესახებ"

#: data/installer.metainfo.xml.in:87
msgid "Double the boot partition size for encrypted installs"
msgstr "დაშიფრული დისკებისთვის ჩამტვირთავი დანაყოფის ზომა ორჯერ გაიზარდა"

#: data/installer.metainfo.xml.in:97
msgid "Warn about installing in a VM"
msgstr "გაფრთხლება VM-ში დაყენების შესახებ"

#: data/installer.metainfo.xml.in:107
msgid "Avoid political naming of certain locales"
msgstr "ზოგიერთი ენის პოლიტიკური სახელების თავიდან აცილება"

#: data/installer.metainfo.xml.in:108
msgid "Fix styles for \"Unused\" disk partitions"
msgstr "\"გამოუყენებელ\" დისკის დანაყოფების სტილის გასწორება"

#: data/io.elementary.installer.desktop.in:4
msgid "Install the system"
msgstr "სისტემის დაყენება"
